    <animation>
        <type>pick</type>
	<object-name>ATHR.off</object-name>
        <action>
            <button>0</button>
	    <repeatable>false</repeatable>
			<binding>
			<command>property-assign</command>
			<property>controls/switches/at</property>
			<value>1</value>
			</binding>
			<binding>
			<command>nasal</command>
			<script>
			if (getprop("/autopilot/locks/at-mode") == 0) {
				setprop("/autopilot/locks/speed", "speed-with-throttle");
			} else if (getprop("/autopilot/locks/at-mode") == 1) {
				setprop("/autopilot/locks/speed", "mach-with-throttle");
			}
			</script>
			</binding>
        </action>
    </animation>
	
    <animation>
        <type>pick</type>
	<object-name>ATHR.on</object-name>
        <action>
            <button>0</button>
	    <repeatable>false</repeatable>
			<binding>
			<command>property-assign</command>
			<property>controls/switches/at</property>
			<value>0</value>
			</binding>
			<binding>
			<command>property-assign</command>
			<property>autopilot/locks/speed</property>
			<value></value>
			</binding>
        </action>
    </animation>
	
	<animation>
        <type>select</type>
        <object-name>ATHR.off</object-name>
        <condition>
            <equals>
                <property>controls/switches/at</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>
	
	<animation>
        <type>select</type>
        <object-name>ATHR.on</object-name>
        <condition>
            <equals>
                <property>controls/switches/at</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>
  
